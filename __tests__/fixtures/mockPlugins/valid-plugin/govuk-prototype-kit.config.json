	  },
	  {
		"name": "Start page with step by step",
		"path": "/templates/start-with-step-by-step.html",
		"type": "nunjucks"
	  }
	],
	"pluginDependencies": [
	  {
	    "packageName": "govuk-frontend",
		  "minVersion": "4.5.0",
		  "maxVersion": "5.5.0"
	  }
	]
  }